% Generic security parameter without and with indexing.
\newcommand{\secp}{n}
\newcommand{\secpi}[1]{\secp_{#1}}
\newcommand{\secpl}[1]{\secp_{\securityparamstyle{#1}}}

% Unary encoding of security parameter.
\newcommand{\usecp}{\allones{\secp}}

% Security parameter for capturing statistically small deficiencies in
% distributions.
\newcommand{\secpr}{\secpl{r}}

% Generic experiment used to define the security of a cryptographic
% primitive.
\newcommand{\Experi}[3]{\mathsf{Exp}_{#1}^{#2}(#3)}

% Generic state between invokations of steps in experiment.
\newcommand{\state}{\mathit{state}}

% Probability that variables are equal. This is often used in
% definitions of security, where forgetting "= 1" is common.
\newcommand{\Prob}[2]{\pPr{#1=#2}}

% Generic adversary.
\newcommand{\Adv}{\mathcal{A}}
\newcommand{\Advi}[1]{\Adv_{#1}}
\newcommand{\Advl}[1]{\Adv_{\labelstyle{#1}}}

