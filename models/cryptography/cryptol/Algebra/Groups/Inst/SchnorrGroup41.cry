module Algebra::Groups::Inst::SchnorrGroup41 =
       Algebra::Groups::SchnorrGroup where

// Overgroup prime modulues
type p = 83

// Schorr subgroup order
type q = 41

// Cofactor of the subgroup
type r = 2

// Used to produce the generator
type h = 16
